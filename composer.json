{
  "name": "blog/app",
  "type": "project",
  "description": "The amazing blog management side project platform .",
  "keywords": [
    "blog",
    "management"
  ],
  "require": {
    "php": "^8.2",
    "doctrine/dbal": "^3.2",
    "doctrine/orm": "^2.11.0",
    "ramsey/uuid": "^4.7",
    "symfony/cache": "^5.4",
    "symfony/yaml": "^5.4",
    "ext-iconv": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "10.3.5",
    "yiisoft/var-dumper": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
